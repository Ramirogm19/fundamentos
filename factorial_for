#include "stdafx.h"
#include<iostream>
#include"conio.h"

using namespace std;
void main()

{
	float n,fac,j;
	cout<<"inrese el valor del que sacara el factorial"<<"\n";
	cin>>n;
	fac=1;	
	for (int i=1;i<=n;i++)
		{
			fac=fac*i;
		}	
	cout<<"su resultado es: "<<"\n";
	cout<<fac;
	getch();
}

