#include "stdafx.h"
#include<iostream>
#include"conio.h"

using namespace std;
void main()

{
	int n,fac,i,j;
	cout<<"inrese el valor del que sacara el factorial"<<"\n";
	cin>>n;
	fac=1;
	i=1;	
	while (i<=n)
		{
			fac=fac*i;
			i=i+1;	
		}	
	cout<<"su resultado es: "<<"\n";
	cout<<fac;
	getch();
}

